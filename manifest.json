{
  "manifest_version": 3,
  "name": "Greeting New Tab",
  "version": "1.0",
  "description": "Shows a greeting with the current user's name and profile name.",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Settings"
  },
  "icons": {
    "48": "icon.png"
  }
}
